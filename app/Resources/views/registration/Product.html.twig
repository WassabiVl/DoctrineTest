{% extends 'base.html.twig' %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.ProductName) }}
    {{ form_row(form.ProductPrice) }}
    {{ form_row(form.ProductDescription) }}
    {{ form_row(form.CategoryName) }}
    {{ form_widget(form.submit, { 'label': 'Save' }) }}
    {{ form_widget(form.reset, { 'label': 'Reset' }) }}
    {{ form_end(form) }}
{% endblock %}